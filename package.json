{
  "name": "authentication-server",
  "version": "0.1.0",
  "description": "authentication and authorization server for web apps",
  "main": "app.js",
  "scripts": {
    "start": "set NODE_ENV=production&& node ./bin/www",
    "dev": "nodemon ./bin/www",
